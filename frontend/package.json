{
  "name": "help-motivate-me-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:types": "python3 ../scripts/fix-openapi-spec.py ../openapi/v1.json && openapi-typescript ../openapi/v1.json -o src/lib/types/generated/api.d.ts --root-types --root-types-no-schema-prefix",
    "generate:spec": "curl -s http://localhost:5001/openapi/v1.json > ../openapi/v1.json && python3 ../scripts/fix-openapi-spec.py ../openapi/v1.json && echo 'OpenAPI spec updated'"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/forms": "^0.5.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.0.0",
    "@vite-pwa/sveltekit": "^1.1.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.3.0",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.54.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^1.2.0",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "type": "module",
  "dependencies": {
    "canvas-confetti": "^1.9.4",
    "driver.js": "^1.4.0",
    "svelte-i18n": "^4.0.1"
  }
}
